<?xml version='1.0' encoding='utf-8'?>
<widget id="com.maxprog.songbooks" version="0.15.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Spiewnik Pielgrzyma i Spiewnik Wedrowca</name>
    <description>Śpiewnik Pielgrzyma i Wędrowca</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashReloadOnOrientationChange" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <icon platform="android" src="res/icon/ios/icon.png" />
    <icon density="land-ldpi" platform="android" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="land-mdpi" platform="android" src="res/icon/android/icon-48-mdpi.png" />
    <icon density="land-hdpi" platform="android" src="res/icon/android/icon-72-hdpi.png" />
    <icon density="land-xhdpi" platform="android" src="res/icon/android/icon-96-xhdpi.png" />
    <splash density="port-ldpi" platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
    <splash density="port-hdpi" platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash density="port-xhdpi" platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
    <icon platform="ios" src="res/icon/ios/icon.png" />
    <icon height="57" platform="ios" src="res/icon/ios/icon-57.png" width="57" />
    <icon height="72" platform="ios" src="res/icon/ios/icon-72.png" width="72" />
    <icon height="114" platform="ios" src="res/icon/ios/icon-57-2x.png" width="114" />
    <icon height="144" platform="ios" src="res/icon/ios/icon-72-2x.png" width="144" />
    <splash height="480" platform="ios" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="640" platform="ios" src="res/screen/ios/screen-iphone-landscape-2x.png" width="960" />
    <splash height="320" platform="ios" src="res/screen/ios/screen-iphone-landscape.png" width="480" />
    <splash height="1136" platform="ios" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1024" platform="ios" src="res/screen/ios/screen-ipad-portrait.png" width="768" />
    <splash height="2048" platform="ios" src="res/screen/ios/screen-ipad-portrait-2x.png" width="1536" />
    <splash height="768" platform="ios" src="res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <splash height="1546" platform="ios" src="res/screen/ios/screen-ipad-landscape-2x.png" width="2048" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.5.4" />
    <engine name="android" spec="~7.1.0" />
</widget>
